 public static void main(String[] args){


 int[] solution = new int[64];
 for(int i=0;i<64;i++){
 solution[i]=0;
 }
 for(int j=0;j<100000000;j++){
 int[] newSolution = new int[64];
 for(int i=0;i<64;i++){
 newSolution[i]=solution[i];
 }
 for(int i=0;i<32;i++){
 int random=(int)(Math.random()*64);
 int sign=(int)(Math.random()*3);
 newSolution[random]=sign;
 }
 if(calculateDifference(newSolution)<calculateDifference(solution)){
 solution=newSolution;
 }
 }
 System.out.println(calculateDifference(solution));
 for(int i=0;i<64;i++){
 System.out.print(solution[i]);
 }
 System.out.println();

 }

 public static long calculateDifference(int[] solution){
 long difference=0;
 for(int i=0;i<solution.length;i++){
 if(solution[i]==1){
 difference-=array[i];
 }else if(solution[i]==2){
 difference+=array[i];
 }
 }
 if(difference==0){
 return Long.MAX_VALUE;
 }
 return Math.abs(difference);
 }


}
